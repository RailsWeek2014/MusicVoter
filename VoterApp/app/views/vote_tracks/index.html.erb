<h1>Listing vote_tracks</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Track</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @vote_tracks.each do |vote_track| %>
      <tr>
        <td><%= vote_track.user_id %></td>
        <td><%= vote_track.track_id %></td>
        <td><%= link_to 'Show', vote_track %></td>
        <td><%= link_to 'Edit', edit_vote_track_path(vote_track) %></td>
        <td><%= link_to 'Destroy', vote_track, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Vote track', new_vote_track_path %>
